{
  "ServerStarters:Scheme": "https",
  "ServerStarters:Host": "serverstarter.balancemod.tf",
  "ServerStarters:ServerInfoBaseAddress": "https://api.balancemod.tf/",
  "ServerStarters:Timings": {
    "Interval": "00:00:10.000",
    "CacheLength": "00:00:10.000"
  },
  "ServerStarters:ForwardedHeaders": {
    "Headers": 7,
    "Limit": "1",
    "KnownNetworks": [
      {
        "Prefix": "10.0.0.0",
        "PrefixLength": 8
      }
    ]
  },
  "ServerStarters:Elastic:Url": "http://elastic:9200",
  "ServerStarters:Elastic:Username": "log_writer",
  "ServerStarters:Elastic:ApmEnabled": true,
  "ElasticApm": {
    "ServerUrls": "http://elasticapm:8200",
    "TransactionSampleRate": 1.0
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithElasticApmCorrelationInfo"
    ],
    "Properties": {
      "Application": "ServerStarter.Server"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.clef",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit ": true,
          "fileSizeLimitBytes": "5242880"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit ": true,
          "fileSizeLimitBytes": "5242880"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "IdentityServer": {
    "Key": {
      "Type": "File",
      "FilePath": "signing/tempkey.json",
      "Password": ""
    },
    "Clients": {
      "ServerStarter.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Databases": {
      "ServerStarter": {
        "ConnectionString": "",
        "Provider": "MySQL",
        "Meta": "10.3.15-MariaDB-1"
      }
    }
  }
}